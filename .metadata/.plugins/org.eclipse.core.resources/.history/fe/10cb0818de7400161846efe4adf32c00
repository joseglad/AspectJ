package exercice2.etape1;

public class Order {
	private int id;
	private double amount;
	private Client client;
	
	public void setClient(Client c){
		this.client = c;
	}
	
	public Client getClient() {
		return this.client;
	}
	
	public void printOrder(){
		System.out.println("id : " + id + "; amount : " + amount + "; client : " + client);
	}
}
