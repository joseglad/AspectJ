package exercice2.etape1;

public class Client {
	private String name;
	private String address;
	//private Orders orders;
	
	public Client(String name, String address){
		this.name = name;
		this.address = address;
	}
	
	public String toString(){
		return this.name;
	}
	
	public String getAddress(){
		return this.address;
	}
}
